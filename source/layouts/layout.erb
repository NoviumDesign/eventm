<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "The Middleman" %></title>
    
    <%= stylesheet_link_tag "app", "style" %>
    <%= javascript_include_tag "modernizr" %>
  </head>
  
  <body class="<%= page_classes %>">

    <div class="off-canvas-wrap">
      <div class="inner-wrap">

        <%= partial "/layouts/partials/sidemenus" %>
        <%= partial "/layouts/partials/header" %>
        <%= yield %>
        <%= partial "/layouts/partials/profileside" %>
        <%= partial "/layouts/partials/footer" %>

      </div>
    </div>
    
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.0/jquery.min.js"></script>
    <%= javascript_include_tag  "all" %>
  </body>
</html>
